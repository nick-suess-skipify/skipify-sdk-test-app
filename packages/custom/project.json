{
    "name": "custom",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "packages/custom/src",
    "projectType": "library",
    "targets": {
        "build": {
            "executor": "@nx/vite:build",
            "outputs": [
                "{options.outputPath}"
            ],
            "options": {
                "outputPath": "dist/custom"
            },
            "dependsOn": [
                "^build",
                "shared:build",
                "checkout-button:build"
            ],
            "configurations": {
                "local": {
                    "mode": "development",
                    "fileReplacements": [
                        {
                            "replace": "packages/shared/src/environments/environment.ts",
                            "with": "packages/shared/src/environments/environment.local.ts"
                        }
                    ]
                },
                "dev": {
                    "mode": "development",
                    "fileReplacements": [
                        {
                            "replace": "packages/shared/src/environments/environment.ts",
                            "with": "packages/shared/src/environments/environment.dev.ts"
                        }
                    ]
                },
                "stage": {
                    "mode": "staging",
                    "fileReplacements": [
                        {
                            "replace": "packages/shared/src/environments/environment.ts",
                            "with": "packages/shared/src/environments/environment.stage.ts"
                        }
                    ]
                },
                "prod": {
                    "mode": "production",
                    "fileReplacements": [
                        {
                            "replace": "packages/shared/src/environments/environment.ts",
                            "with": "packages/shared/src/environments/environment.prod.ts"
                        }
                    ]
                }
            }
        },
        "lint": {
            "executor": "@nx/eslint:lint",
            "outputs": [
                "{options.outputFile}"
            ],
            "options": {
                "lintFilePatterns": [
                    "packages/custom/**/*.ts"
                ]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
                "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
                "jestConfig": "packages/custom/jest.config.ts",
                "passWithNoTests": true
            },
            "configurations": {
                "ci": {
                    "ci": true,
                    "codeCoverage": true
                }
            }
        }
    },
    "tags": []
}